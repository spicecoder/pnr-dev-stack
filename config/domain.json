{
    "name": "dev_stack",
    "env": {
        "mongodb": {
            "url": "mongodb://localhost:27017",
            "options": {}
        }
    },
    "cpuxs": {
        "stack_startup": {
            "id": "stack_startup",
            "design_chunks": [
                {
                    "name": "mongodb",
                    "gatekeeper": {
                        "system_ready": {
                            "prompt": "Is system ready?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "flowout": {
                        "mongodb_status": {
                            "prompt": "Is MongoDB running?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "status": "ready",
                    "container": {
                        "name": "pnr_mongodb",
                        "image": "mongo:latest",
                        "ports": {
                            "27017": "27017"
                        }
                    }
                },
                {
                    "name": "mongodb_checker",
                    "gatekeeper": {
                        "mongodb_status": {
                            "prompt": "Is MongoDB running?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "flowout": {
                        "mongodb_connected": {
                            "prompt": "Is MongoDB connected?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "status": "ready",
                    "container": {
                        "name": "pnr_mongodb_checker",
                        "image": "pnr_mongodb_checker:latest",
                        "build_path": "nodes/mongodb"
                    }
                },
                {
                    "name": "api_server",
                    "gatekeeper": {
                        "mongodb_connected": {
                            "prompt": "Is MongoDB connected?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "flowout": {
                        "api_ready": {
                            "prompt": "Is API server ready?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "status": "ready",
                    "container": {
                        "name": "pnr_api_server",
                        "image": "pnr_api:latest",
                        "build_path": "nodes/api",
                        "ports": {
                            "3000": "3000"
                        }
                    }
                },
                {
                    "name": "web_client",
                    "gatekeeper": {
                        "api_ready": {
                            "prompt": "Is API server ready?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "flowout": {
                        "client_ready": {
                            "prompt": "Is web client ready?",
                            "response": ["yes"],
                            "tv": "Y"
                        }
                    },
                    "status": "ready",
                    "container": {
                        "name": "pnr_web_client",
                        "image": "pnr_web:latest",
                        "build_path": "nodes/client",
                        "ports": {
                            "80": "8080"
                        }
                    }
                }
            ]
        }
    }
}